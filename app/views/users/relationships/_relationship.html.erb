<div class="follower">
  <% users.each do |user| %>
    <%= link_to user_path(user) do %>
    	<%= attachment_image_tag user, :profile_image, size: "50x50", format: 'jpeg', fallback: "no_image.jpg" %>
  		name / <%= user.name %>
  	<% end %>
  	<%= render 'follow_button', user: user %>
  <% end %>
</div>